{
    "name": "dmzx/topicauthor",
    "type": "phpbb-extension",
    "description": "This extension will add the topic author to the viewtopic.",
    "homepage": "http://www.dmzx-web.net",
    "version": "2.0.0",
    "time": "2020-04-24",
    "keywords": [
        "phpbb",
        "extension",
        "topicauthor"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "dmzx",
            "homepage": "http://www.dmzx-web.net",
            "email": "info@dmzx-web.net",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "extra": {
        "display-name": "Topic Author",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.4.0@dev"
        },
        "version-check": {
            "host": "www.dmzx-web.net",
            "directory": "/versions",
            "filename": "topicauthor_version.json"
        }
    }
}
